  <ellipse cx="40" cy="36" rx="1" ry="2" fill="#ea9999"/>
  
  <!-- Mouth -->
  <path d="M 37 40 Q 40 42 43 40" stroke="#000" stroke-width="1" fill="none"/>
  
  <!-- Energy symbol on shirt -->
  <circle cx="40" cy="80" r="6" fill="#ffeb3b" stroke="#ffc107" stroke-width="1"/>
